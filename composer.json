{
    "name": "kiboko/fatture-cloud-laravel",
    "description": "Wrapper API fatture in cloud Laravel",
    "keywords": ["laravel", "fatture-cloud", "invoices"],
	"authors": [
        {
            "name": "Matteo Schiona",
            "email": "schiona.matteo@gmail.com",
            "homepage": "http://www.kiboko.it",
            "role": "Backend Developer"
        }
    ],
    "autoload": {
        "psr-4": {
            "Kiboko\\FattureCloud\\": "src/Kiboko/FattureCloud"
        }
    },
    "require": {
        "php": ">=5.4.0",
        "illuminate/database": "4.* | 5.*",
        "illuminate/support": "4.* | 5.*"
    },
    "require-dev": {
        "phpunit/phpunit": "4.*",
        "phpspec/phpspec": "~2.0"
    },
    "autoload-dev": {
        "psr-4": {
            "Kiboko\\FattureCloud\\Tests\\": "tests/"
        }
    }
}
